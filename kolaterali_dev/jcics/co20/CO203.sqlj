		    					USE_ID,
		    					OPENING_TS,
		    					USER_LOCK,
		    					BANK_SIGN
		    					)
		    				VALUES (
			    				:(decl.resubtypeupdatem.RE_SUB_TYPE_ID_a), 
								:(decl.resubtypeupdatem.RE_SUB_TYPE_CODE_a), 
								:(decl.resubtypeupdatem.RE_SUB_TYPE_DESC_a),
								:(decl.resubtypeupdatem.RE_TYPE_ID_a),
