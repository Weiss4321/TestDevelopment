			 //provjerava da li vec postoji isti vlasnik
			#sql [ctx] {SET TRANSACTION ISOLATION LEVEL READ COMMITTED};
			 		
    
			
			try{	
		    	#sql [ctx]{  insert into re_sub_type_a (
